---
import PortfolioView from "@components/art/PortfolioView.astro";
import Layout from "@layouts/Layout.astro";
import { getCollection } from "astro:content";

const items = await getCollection("art");

items.sort((a, b) => a.data.date.getTime() - b.data.date.getTime());
---

<Layout title="Art">
  <PortfolioView {items} />
</Layout>
